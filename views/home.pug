#modal-overlay.game-setup(style="display:none")
  #modal-wrap
    span.close(data-icon='L')
    div#createoverlay
      h2 Create a game
      form(method='post', action='/start')
        .variant.label_select
          label(for='sf_variant') Variant
          select#sf_variant(name='variant')
            option(value='1', title='Standard rules of chess (FIDE)', selected='selected') Standard
            //
              option(value='10', title='Captured pieces can be dropped back on the board instead of moving a piece.') Crazyhouse
              option(value='2', title='Starting position of the home rank pieces is randomized.') Chess960
              option(value='4', title='Bring your King to the center to win the game.') King of the Hill
              option(value='5', title='Check your opponent 3 times to win the game.') Three-check
              option(value='6', title='Lose all your pieces (or get stalemated) to win the game.') Antichess
              option(value='7', title="Nuke your opponent's king to win.") Atomic
              option(value='8', title='Destroy the horde to win!') Horde
              option(value='9', title='Race your King to the eighth rank to win.') Racing Kings
        .time_mode_config.optional_config
          .label_select
            label(for='sf_timeMode') Time control
            select#sf_timeMode(name='timeMode')
              option(value='1') Real time
              option(value='0',selected='selected') Unlimited
          .time_choice.range(style='display: none;')
            | Minutes per side: 
            span 5
            input.range-value(name='time', value='5.0', type='hidden')
            input.range(name='time_range', type='range', min='0', max='38', value='9')
          .increment_choice.range(style='display: none;')
            | Increment in seconds: 
            span 3
            input.range-value(name='increment', value='3', type='hidden')
            input.range(name='increment_range', type='range', min='0', max='30', value='3')
        .mode_choice.buttons
          group.radio
            div
              input#sf_mode_0(type='radio', name='views', value='public',checked='checked')
              label.required(for='sf_mode_0') Public
            div
              input#sf_mode_1(type='radio', name='views', value='private')
              label.required(for='sf_mode_1') Private
        .optional_config
          .rating-range-config
            | Rating range
            .rating-range
              input(name='ratingRange', value='600-2900', type='hidden')
              input.range.rating-range__min(name='ratingRange_range_min', type='range', min='-500', max='0', step='50', value='-500')
              span.rating-min -500
              | /
              span.rating-max +500
              input.range.rating-range__max(name='ratingRange_range_max', type='range', min='0', max='500', step='50', value='500')
        .color-submits
          button.color-submits__button.button.button-metal.black(type='submit', title='Black', name='color', value='black')
            i
          button.color-submits__button.button.button-metal.random(type='submit', title='Random side', name='color', value='random')
            i
          button.color-submits__button.button.button-metal.white(type='submit', title='White', name='color', value='white')
            i
      .ratings
        input(name='rating', value='1500', type='hidden')
        .ultraBullet(style='display: none;')
          | Rating: 
          strong(data-icon='{') 1500
          |  UltraBullet
        .bullet(style='display: none;')
          | Rating: 
          strong(data-icon='T') 1500
          |  Bullet
        .blitz(style='display: none;')
          | Rating: 
          strong(data-icon=')') 1769
          |  Blitz
        .rapid(style='display: none;')
          | Rating: 
          strong(data-icon='#') 1752
          |  Rapid
        .classical(style='display: none;')
          | Rating: 
          strong(data-icon='+') 1500
          |  Classical
        .correspondence(style='display: block;')
          | Rating: 
          strong(data-icon=';') 1500
          |  Correspondence
        .crazyhouse(style='display: none;')
          | Rating: 
          strong(data-icon='') 1500
          |  Crazyhouse
        .chess960(style='display: none;')
          | Rating: 
          strong(data-icon="'") 1500
          |  Chess960
        .kingOfTheHill(style='display: none;')
          | Rating: 
          strong(data-icon='(') 1500
          |  King of the Hill
        .threeCheck(style='display: none;')
          | Rating: 
          strong(data-icon='.') 1500
          |  Three-check
        .antichess(style='display: none;')
          | Rating: 
          strong(data-icon='@') 1500
          |  Antichess
        .atomic(style='display: none;')
          | Rating: 
          strong(data-icon='>') 1500
          |  Atomic
        .horde(style='display: none;')
          | Rating: 
          strong(data-icon='_') 1500
          |  Horde
        .racingKings(style='display: none;')
          | Rating: 
          strong(data-icon='') 1500
          |  Racing Kings
          
    div#joinoverlay
      h2 Play with a friend
      form(method='post', action='/setup/friend', novalidate='novalidate', data-random-color-variants='5,6,9,7,8', data-type='friend')
        .fen_position.optional_config(style='display: block;')
          .fen_form(data-validate-url='/setup/validate-fen')
            input#form3-fen.form-control(name='fen', placeholder='Paste the FEN text here')
            a.button.button-empty(data-icon='m', title='Board editor', href='/editor')
          a.board_editor(href='/editor')
            span.preview
    
header#top
  .site-title-nav
    input#tn-tg.topnav-toggle.fullscreen-toggle(type='checkbox' autocomplete='off' aria-label='Navigation')
    label.fullscreen-mask(for='tn-tg')
    label.hbg(for='tn-tg')
      span.hbg__in
    h1.site-title
      a(href='/')
        | lichess
        span .org
    nav#topnav.hover
      section
        a(href='/')
          span.play Play
          span.home lichess.org
        div(role='group')
          a(href='/?any#hook') Create a game
      section
        a(href='/practice') Learn
        div(role='group')
          a(href='/learn') Tutorials
          a(href='https://www.youtube.com/watch?v=eqMEiBu4M1c') Videos
      section
        a(href='/analysis') Tools
        div(role='group')
          a(href='/analysis') Analysis board
          a(href='/editor') Board editor
          a(href='/paste') Import game
  .site-buttons
    #clinput
      a.link
        span(data-icon='y')
      input(spellcheck='false' autocomplete='false' aria-label='Search' placeholder='Search')
    div
      a#challenge-toggle.toggle.link
        span.data-count(title='Challenges' data-count='0' data-icon='U')
      #challenge-app.dropdown
    div
      a#notify-toggle.toggle.link
        span.data-count(title='Notifications' data-count='0' data-icon='')
      #notify-app.dropdown
    .dasher
      a#user_tag.toggle.link(href='/logout') Player
      #dasher_app.dropdown
  
